{
	"name": "@prismicio/types",
	"version": "0.2.4",
	"description": "Type definitions for Prismic related structure",
	"keywords": [
		"typescript",
		"types",
		"utils",
		"toolbox",
		"prismic"
	],
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/prismicio/prismic-types.git"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./graphql": {
			"require": "./dist/graphql/index.cjs",
			"import": "./dist/graphql/index.js"
		},
		"./dist/graphql": {
			"require": "./dist/graphql/index.cjs",
			"import": "./dist/graphql/index.js"
		},
		"./package.json": "./package.json"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"react-native": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "siroc build",
		"dev": "siroc build --watch",
		"format": "prettier --write .",
		"lint": "eslint --ext .js,.ts .",
		"prepare": "npm run build",
		"release": "npm run build && npm run test && standard-version && git push --follow-tags && npm run build && npm publish",
		"release:alpha": "npm run build && npm run test && standard-version --release-as patch --prerelease alpha && git push --follow-tags && npm run build && npm publish --tag alpha",
		"release:alpha:dry": "standard-version --release-as patch --prerelease alpha --dry-run",
		"release:dry": "standard-version --dry-run",
		"size": "size-limit",
		"test": "npm run lint && npm run types && npm run unit",
		"types": "tsc --noEmit",
		"unit": "vitest run --coverage",
		"unit:watch": "vitest watch"
	},
	"devDependencies": {
		"@prismicio/types-internal": "^1.4.1",
		"@size-limit/preset-small-lib": "^8.1.0",
		"@typescript-eslint/eslint-plugin": "^5.43.0",
		"@typescript-eslint/parser": "^5.43.0",
		"@vitest/coverage-c8": "^0.25.2",
		"eslint": "^8.27.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-tsdoc": "^0.2.17",
		"prettier": "^2.7.1",
		"prettier-plugin-jsdoc": "^0.4.2",
		"siroc": "^0.16.0",
		"size-limit": "^8.1.0",
		"standard-version": "^9.5.0",
		"ts-expect": "^1.3.0",
		"typescript": "^4.9.3",
		"vitest": "^0.25.2"
	},
	"engines": {
		"node": ">=12.7.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
